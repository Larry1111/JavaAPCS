import java.time.DayOfWeek;
import java.util.Scanner; 
public class dayOfWeeek {
	private static int n;
	private static int m;
	public static void dayOfWeek(int day, int date) {
		day = n;
		date = m;
	}
public static void main(String[] args) {
			String dayString = null;
			Scanner s = new Scanner(System.in);
			System.out.println("Enter the day of the week on which the month begins: ");
			int day1 = s.nextInt();
			System.out.println("Enter the date in the month that is being found: ");
			int dayOfWeek1 = s.nextInt();
			int dayOfWeek = (day1 - (7 - dayOfWeek1 + 1) + 7) %7;
			switch(dayOfWeek) {
			case 1:
				dayString = "Monday";
			case 2: 
	            dayString = "Tuesday"; 
	            break; 
	        case 3: 
	            dayString = "Wednesday"; 
	            break; 
	        case 4: 
	            dayString = "Thursday"; 
	            break; 
	        case 5: 
	            dayString = "Friday"; 
	            break; 
	        case 6: 
	            dayString = "Saturday"; 
	            break; 
			}
			
			System.out.println("The day of the week for January " + day1 + " is "+ dayString);	
}

}
