
public class Assignment {
	
//1
	public static int product(int a, int b) {
		if(b == 1) {
			return a;
		}
		else {
			return a + product(a, b-1);
		}
	}
	
//3
	public static String process(String str) {
		int n = str.length();
		if(n>=2) {
			int n2 = n/2;
			str = process(str.substring(n2))+
					process(str.substring(0, n2));
			
		}
		return str;
	}
//5 a,b,c,e,g,h
//7 00100123
//10
	//public int gcf(int a, int b) {
		
	//}
//11
//14
	public static long fibonacci(int n) {
		int sum = 1;
		if(n<=2) {
			sum = 1;
		}
		for(int i = 1; i < n; i++) {
			sum += sum*(i-1)+i;
		}
	}
	
//15
//17
//18
	public static double continuedFrac(int n) {
		if(n==1) {
			return 1;
		}
		else {
			return 1 + 1/continuedFrac(n-1);
		}
	}
	public static void main(String[] args) {
		System.out.println(product(3,4));
		System.out.println(continuedFrac(14));
		System.out.println(process("HAVE")+" "+process("FUN"));
	}
}
