			//22
			char[] hello = {' ', 'h', 'e', 'l','l','o'};
			int i= 0;
			while(i<6)
			{
				i++;
				hello[i-1] = hello[i];
			}
			hello[5]='!';
			//23
			public static boolean isMedian(double[] sample, int m)
			{
				int countLess = 0;
				int countGreater = 0;
				for(int i=0; i < sample.length;i++)
				{
					if(sample[i]> m)
					{
						countGreater++;
					}
					if(sample[i]<m)
					{
						countLess++;
					}
				if (countLess == countGreater) {
					return true;
				}
				else
					return false;
				}
			}
			public static void main(String[] arg) {
				Examples.printIntArray.createWedge(5);
			}
}}
			
			

